{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint", "^prettier", "^typecheck"]
    },
    "lint:fix": {
      "dependsOn": ["^lint:fix", "prettier:fix"]
    },
    "prettier": {
      "dependsOn": ["^prettier"]
    },
    "prettier:fix": {
      "dependsOn": ["^prettier:fix"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
