{
  "name": "@code-dot-org/marketing-storybook",
  "private": true,
  "keywords": [
    "marketing",
    "code.org"
  ],
  "homepage": "https://github.com/code-dot-org/code-dot-org/blob/staging/frontend/apps/marketing-storybook/#readme",
  "bugs": {
    "url": "https://github.com/code-dot-org/code-dot-org/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-dot-org/code-dot-org.git",
    "directory": "frontend/apps/marketing-storybook"
  },
  "license": "SEE LICENSE IN LICENSE",
  "type": "module",
  "scripts": {
    "build": "storybook build -o dist/marketing-storybook",
    "build-storybook": "storybook build",
    "clean": "rimraf dist",
    "dev": "yarn run storybook",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "storybook": "storybook dev -p 6007",
    "test-storybook": "vitest --project=storybook",
    "test:ui:ci": "yarn test-storybook",
    "test:ui:local": "test-storybook --url http://localhost:6006"
  },
  "prettier": "@code-dot-org/lint-config/prettier/index.mjs",
  "stylelint": {
    "extends": "@code-dot-org/lint-config/stylelint/index.mjs"
  },
  "devDependencies": {
    "@applitools/eyes-storybook": "^3.56.0",
    "@chromatic-com/storybook": "^4.0.1",
    "@code-dot-org/component-library": "workspace:*",
    "@code-dot-org/component-library-styles": "workspace:*",
    "@code-dot-org/fonts": "workspace:*",
    "@code-dot-org/lint-config": "workspace:*",
    "@mdx-js/react": "^3.1.0",
    "@storybook/addon-a11y": "9.0.18",
    "@storybook/addon-docs": "9.0.18",
    "@storybook/addon-themes": "9.0.18",
    "@storybook/addon-vitest": "9.0.18",
    "@storybook/nextjs-vite": "9.0.18",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.16.0",
    "eslint-plugin-storybook": "9.0.18",
    "markdown-to-jsx": "^7.7.12",
    "playwright": "~1.55.1",
    "prettier": "3.4.2",
    "storybook": "9.0.18",
    "vitest": "^3.2.4"
  }
}
